{
  "name": "ra-data-dexie",
  "version": "1.0.9",
  "description": "Experimental React-Admin using Dexie (IndexedDB)",
  
  "main": "src/index.js",
  "module": "src/index.js",

  "repository": {
    "type": "git",
    "url": "git+https://github.com/tykoth/ra-data-dexie.git"
  },
  "keywords": [
    "reactjs",
    "react",
    "react-admin",
    "local",
    "indexeddb"
  ],
  "author": "Thiago Bocchile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tykoth/ra-data-dexie/issues"
  },
  "homepage": "https://github.com/tykoth/ra-data-dexie#readme",

"scripts": {

  "build": "rimraf ./lib && cross-env NODE_ENV=production BABEL_ENV=cjs babel --quiet ./src -d ./lib --ignore spec.js,test.js",
  "build-esm": "rimraf ./esm && cross-env NODE_ENV=production BABEL_ENV=esm babel --quiet ./src -d ./esm --ignore spec.js,test.js",
  "watch": "rimraf ./lib && cross-env NODE_ENV=production babel --watch ./src -d ./lib --ignore spec.js,test.js"

},
"dependencies": {
  "babel-plugin-transform-object-rest-spread": "^6.26.0",
  "lodash": "^4.17.11",
  "query-string": "~5.1.1",
  "react-admin": "^2.3.1",
  "dexie": "^2.0.4"
},
"devDependencies": {
    "cross-env": "^5.2.0",
    "babel-cli": "~6.26.0",
    "rimraf": "^2.6.2"
}
}
